public class Percentages {
    public static void main (String args[]) 
    {
        double num1 = 2.0;
        double num2 = 5.0;
        
        computePercent(num1, num2);
        computePercent(num2, num1);
    }
    public static void computePercent(double myNumber1, double myNumber2) 
    {
        double percent1 = (myNumber1/myNumber2)*100;
        System.out.println(myNumber1 + " is " + percent1 + "% of " + myNumber2);
    }
}
